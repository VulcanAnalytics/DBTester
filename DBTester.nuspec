<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>VulcanAnalytics.DBTester</id>
    <version>0.7.0.0000-alpha</version>
    <authors>Darren Edward Comeau</authors>
    <owners>Darren Edward Comeau</owners>
    <title>Database Testing Library</title>
    <description>A number of simplified methods to check database objects and execute sql against an MSSQL database.</description>
    <tags>database testing mssql sql server</tags>
    <projectUrl>https://github.com/VulcanAnalytics/DBTester/wiki</projectUrl>
    <licenseUrl>https://github.com/VulcanAnalytics/DBTester/blob/master/LICENSE</licenseUrl>

    <releaseNotes>
      Fixed bug in InsertData method, not handling single quotes in data (#17)
      New class VulcanAnalytics.DBTester.ColumnDefaults can be created and passed into the InsertData method so you only need to specify the test relevant columns in a suite of tests (#10)
    </releaseNotes>

    <repository type="git" url="https://github.com/VulcanAnalytics/DBTester.git" />

    <packageTypes>
      <packageType name="Dependency" />
    </packageTypes>

    <dependencies>
      <dependency id="Microsoft.SqlServer.SqlManagementObjects" version="140.17283.0" />
    </dependencies>

    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Data"  />
    </frameworkAssemblies>

  </metadata>
</package>